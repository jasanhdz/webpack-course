    "@babel/preset-es2015": "7.0.0-beta.53",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "6.26.0",
    "css-loader": "2.1.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
